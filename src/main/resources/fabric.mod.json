	"authors": [
		"W"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/fabrichammers/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.w.fabrichammers.FabricHammers"
		],
		"client": [
			"net.w.fabrichammers.FabricHammersClient"
		],
		"fabric-datagen": [
			"net.w.fabrichammers.datagen.FabricHammersDataGenerator"
		]
	},
	"mixins": [
		"fabrichammers.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15",
		"minecraft": "~1.20.4",
		"java": ">=17",
